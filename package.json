{
	"name": "ejs-gen",
	"displayName": "EJS Code Generator",
	"description": "code generator powered by ejs template engine",
	"publisher": "dirty49374",
	"repository": "https://github.com/dirty49374/vscode-ejs-gen",
	"version": "0.0.23",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#ffe04b",
		"theme": "light"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ejs-gen.enable",
				"title": "ejs-gen: enable"
			},
			{
				"command": "ejs-gen.disable",
				"title": "ejs-gen: disable"
			},
			{
				"command": "ejs-gen.generate",
				"title": "ejs-gen: generate current file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/ejs": "^3.0.4",
		"@types/js-yaml": "^3.12.4",
		"@types/node-fetch": "^2.5.7",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"ejs": "^3.1.3",
		"js-yaml": "^3.14.0",
		"glob": "^7.1.6",
		"node-fetch": "^3.1.1"
	}
}
